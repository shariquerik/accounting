 "html": "<!--Header-->\n\n<div class=\"row border-bottom\">\n    <h1 class=\"col-md-6 \">{{ doc.company }}</h1>\n    <div class=\"col-md-6\">\n        <h2 class=\"text-right\">PURCHASE INVOICE</h2>\n        <h5 class=\"text-right text-muted\">{{ doc.name }}</h5>\n    </div>\n</div>\n\n<!--Main-->\n<div class=\"row\">\n    <h6 class=\"col-md-6\">Supplier: <span class=\"text-muted\">{{ doc.party }}</span></h6>\n    <h6 class=\"col-md-6 text-right\">Date: <span class=\"text-muted\">{{ doc.get_formatted('posting_date') }}</span></h6>\n</div>\n<div>\n    <table class=\"table table-bordered\">\n        <thead>\n            <tr>\n                <th scope=\"col\">#</th>\n                <th scope=\"col\">Item</th>\n                <th scope=\"col\">Quantity</th>\n                <th scope=\"col\">Rate</th>\n                <th scope=\"col\">Amount</th>\n            </tr>\n        </thead>\n        <tbody>\n            {% for item in doc.items %}\n            <tr>\n                <th scope=\"row\">{{ item.idx }}</th>\n                <td>{{ item.item }}</td>\n                <td>{{ item.qty }}</td>\n                <td>{{ frappe.format(item.rate, 'Currency') }}</td>\n                <td>{{ frappe.format(item.amount, 'Currency') }}</td>\n            </tr>\n            {% endfor %}\n        </tbody>\n    </table>\n</div>\n<!--Footer-->\n<div class=\"row border-bottom border-top\">\n    <div class=\"col-md-7\">\n        <h6><strong>ACCOUNT DETAILS</strong></h6>\n        <h5>PAID FROM: <span class=\"text-muted\">{{ doc.credit_to }}</span></h5>\n        <h5>PAID TO: <span class=\"text-muted\">{{ doc.expense_account }}</span></h5>\n    </div>\n    <div class=\"col-md-5\">\n        <h6><strong>TOTALS</strong></h6>\n        <h5 class=\"\">TOTAL QUANTITY: <span class=\"text-muted\">{{ doc.total_quantity }}</span></h5>\n        <h5 class=\"\">TOTAL AMOUNT: <span class=\"text-muted\">\u20b9 {{ frappe.format(doc.total_amount, 'Currency') }}</span></h5>\n    </div>\n</div>\n<h1 class=\"text-right\">\u20b9 {{ frappe.format(doc.total_amount, 'Currency') }}/-</h1>",
